{
  "name": "auth-service",
  "version": "1.0.0",
  "description": "美容院SaaS认证服务 - 处理用户注册、登录、JWT颁发和权限管理",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node index.ts",
    "dev:watch": "ts-node-dev --respawn --transpile-only index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "db:reset": "prisma migrate reset --schema=./prisma/schema.prisma",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "auth",
    "authentication",
    "jwt",
    "sso",
    "beauty",
    "saas",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "@sendgrid/mail": "^8.1.5",
    "@types/bcrypt": "^6.0.0",
    "@types/pug": "^2.0.10",
    "@types/validator": "^13.15.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "libphonenumber-js": "^1.12.9",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.4",
    "prisma": "^5.1.1",
    "pug": "^3.0.3",
    "redis": "^5.6.0",
    "speakeasy": "^2.0.0",
    "uuid": "^9.0.0",
    "validator": "^13.15.15",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.2",
    "@types/nodemailer": "^6.4.9",
    "@types/speakeasy": "^2.0.7",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
